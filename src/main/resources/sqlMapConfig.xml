    <sqlMap resource="config/model/sqlmap/Category.xml"/>
    <sqlMap resource="config/model/sqlmap/FacetGroup.xml"/>
    <sqlMap resource="config/model/sqlmap/FacetGroupItem.xml"/>
    <sqlMap resource="config/model/sqlmap/HotkeyIndex.xml"/>
