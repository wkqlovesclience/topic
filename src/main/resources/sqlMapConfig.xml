    <sqlMap resource="config/model/sqlmap/Queue.xml"/>
    <sqlMap resource="config/model/sqlmap/Resource.xml"/>
    <sqlMap resource="config/model/sqlmap/SingleTopRanking.xml"/>
    <sqlMap resource="config/model/sqlmap/SitemapSqlMap.xml"/>
    <sqlMap resource="config/model/sqlmap/static-html.xml"/>
